{
  /* Visit https://firebase.google.com/docs/database/security to learn more about security rules. */
  "rules": {
    "questions": {
      ".read": true,
      ".write": false
    },
    "answers": {
      ".read": true,
      ".write": false
    },
    "users":{
      ".read": true,
      ".write": "auth.uid !== null"
    }
  }
}